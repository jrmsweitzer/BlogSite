<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="driver" value="chrome" />
    <add key="driverDirectory" value="../../Drivers" />
    <!-- Timeout -->
    <add key="defaultTimeout" value="0" />
    <!-- Logging Directories -->
    <add key="logDirectory" value="C:\Selenium\Logs\" />
    <add key="screenshotDirectory" value="C:\Selenium\Results\Screenshots\" />
    <add key="stacktraceDirectory" value="C:\Selenium\Results\" />
    <!-- Logging Names -->
    <add key="seleniumLogName" value="SeleniumLog" />
    <add key="actionlogName" value="Actions" />
    <!-- Logging Bools -->
    <add key="logAllActions" value="true" />
    <add key="logStackTrace" value="true" />
    <add key="takeScreenshotOnTestPass" value="true" />
    <add key="takeScreenshotOnTestFail" value="true" />
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" /></startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-7.0.0.0" newVersion="7.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
